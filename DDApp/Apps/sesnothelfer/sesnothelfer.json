    "PageCompany": {
      "title": "{{company_name?}}",
      "titleImage": "img/icon-6.png",
      "components": [
        {
          "renderType": "Article",
          "provider": {
            "type": "Company",
            "url": "http://ddappservice.doctor-help.com/api/v2/hospital/get-company?guid={{company_guid}}",
            "mapper": [
              {
                "sourceType": "Company",
                "destinationType": "Article",
                "mapping": {
                  "d => d.Style": "s => Article.ImageStyle.HasBorder",
                  "d => d.Sources": "s => s.Pictures.Select(x => x.Thumbnail)",
                  "d => d.Description": "s => s.DescriptionHtml"
                }
              }
            ]
          }
        },
        {
          "renderType": "Item",
          "provider": {
            "type": "Company",
            "url": "http://ddappservice.doctor-help.com/api/v2/hospital/get-company?guid={{company_guid}}",
            "mapper": [
              {
                "sourceType": "Company",
                "destinationType": "Item",
                "mapping": {
                  "d => d.Style": "s => ItemStyle.OneRowItem",
                  "d => d.Title": "s => \"News\"",
                  "d => d.Link": "(s, qc) => $\"PageNews?rss_feed=https://www.ses-stiftung.de/rss.xml\"",
                  "d => d.Icon": "s => \"fas fa-play\""
                }
              }
            ]
          }
        },
        {
          "renderType": "ItemList",
          "provider": {
            "type": "Company",
            "url": "http://ddappservice.doctor-help.com/api/v2/hospital/get-company?guid={{company_guid}}",
            "mapper": [
              {
                "sourceType": "Company",
                "destinationType": "ItemList",
                "mapping": {
                  "d => d.Style": "s => ItemStyle.OneRowItem",
                  "d => d.Subject": "s => \"Kliniken\"",
                  "d => d.Items": "s => s.Hospitals"
                }
              },
              {
                "sourceType": "Hospital",
                "destinationType": "Item",
                "mapping": {
                  "d => d.Link": "(s, qc) => $\"PageHospital?hospital_guid={s.GUID}&hospital_name={s.Name}&rss_feed={(s.GUID == \"260341182\" ? \"https://www.josefshospital-cloppenburg.de/rss.xml\" : (s.GUID == \"260340579\" ? \"https://www.franziskushospital-lohne.de/rss.xml\" : \"https://www.marienhospital-vechta.de/rss.xml\"))}\"",
                  "d => d.Title": "s => s.Name",
                  "d => d.Icon": "s => \"fas fa-play\""
                }
              }
            ]
          }
        },
        {
          "renderType": "ItemList",
          "provider": {
            "type": "Company",
            "url": "http://ddappservice.doctor-help.com/api/v2/hospital/get-company?guid={{company_guid}}",
            "mapper": [
              {
                "sourceType": "Company",
                "destinationType": "ItemList",
                "mapping": {
                  "d => d.Style": "s => ItemStyle.LeftSideImg",
                  "d => d.Subject": "s => s.Personal.Any() ? \"Leitung\" : string.Empty",
                  "d => d.Items": "s => s.Personal"
                }
              },
              {
                "sourceType": "Doctor",
                "destinationType": "Item",
                "mapping": {
                  "d => d.Link": "(s, qc) => $\"PageDoctorFromPersonal?company_guid={qc.company_guid}&doctor_guid={s.GUID}&hospital_name={qc.hospital_name}\"",
                  "d => d.Image": "s => s.Pictures.Select(x => x.Thumbnail).FirstOrDefault() ?? \"/img/image_placeholder.png\"",
                  "d => d.Title": "s => $\"{s.LastName}, {s.FirstName}, {s.Salutation}\"",
                  "d => d.Description": "s => s.Specialty",
                  "d => d.Icon": "s => \"fas fa-play\""
                }
              }
            ]
          }
        }
