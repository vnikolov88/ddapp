                "mapping": {
                  "d => d.Link": "(s, qc) => $\"PageDepartment?hospital_guid={qc.hospital_guid}&department_guid={s.GUID}&hospital_name={qc.hospital_name}\"",
                  "d => d.Title": "s => s.Name",
                  "d => d.Icon": "s => \"fas fa-play\""
                }
