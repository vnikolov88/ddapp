            "reducer": "(x, qc) => x.Departments.FirstOrDefault(department => department.GUID == qc.department_guid)",
            "mapper": [
              {
                "sourceType": "Department",
                "destinationType": "Article",
                "mapping": {
                  "d => d.Style": "s => Article.ImageStyle.HasBorder",
