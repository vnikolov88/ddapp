            "mapper": [
              {
                "sourceType": "Department",
                "destinationType": "ItemList",
                "mapping": {
                  "d => d.Style": "s => s.Certificates.Any(x => x.Name != \"Doctor-Help\") ? ItemStyle.LeftSideImg : ItemStyle.Hidden",
                  "d => d.Subject": "s => \"Zertifikate\"",
                  "d => d.Items": "s => s.Certificates.Where(x => x.Name != \"Doctor-Help\")"
                }
              },
              {
                "sourceType": "Certificate",
                "destinationType": "Item",
                "mapping": {
                  "d => d.Link": "s => s.ExternalUrl.OnDevice()",
                  "d => d.Title": "s => s.Name",
                  "d => d.Image": "s => s.GetThumbnail()",
