            "mapper": [
              {
                "sourceType": "Department",
                "destinationType": "ItemList",
                "mapping": {
                  "d => d.Style": "s => ItemStyle.LeftSideImg",
                  "d => d.Subject": "s => s.Doctors.Any() ? s.PersonalHeadline : string.Empty",
                  "d => d.Items": "s => s.Doctors"
                }
              },
